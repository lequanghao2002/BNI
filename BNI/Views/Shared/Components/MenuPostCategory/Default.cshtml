@model IEnumerable<PostsCategory>
<ul class="navbar-nav">
    <li class="nav-item">
        <span class="nav-link disabled ps-0">Blogs:</span>
    </li>
    <li class="nav-item">
        <a class="nav-link fw-bold active" asp-controller="Blog" asp-action="Index">Tất cả</a>
    </li>
    @foreach (var category in Model)
    {
        <li class="nav-item">
            <a asp-controller="Blog" asp-action="PostinCategory" asp-route-id="@category.Id"
            class="nav-link fw-bold active">@category.Name</a>
        </li>
    }
</ul>